<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>

    <Appenders>
        <Console name="CONSOLE" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{ABSOLUTE} %5p [%-20.20t] %m%n"/>
        </Console>
    </Appenders>

    <Loggers>

        <Logger name="org.williamlake" level="INFO"/>

        <Root level="INFO">
            <AppenderRef ref="CONSOLE"/>
        </Root>

    </Loggers>

</Configuration>